{
  "title": "Token response",
  "type": "object",
  "required": [
    "access_token",
    "access_token_expires_in"
  ],
  "properties": {
    "access_token": {
      "type": "string",
      "readOnly": true
    },
    "access_token_expires_in": {
      "type": "integer",
      "description": "access token expiration time in secondes",
      "readOnly": true
    },
    "refresh_token": {
      "type": "string",
      "readOnly": true
    }
  },
  "$schema": "http://json-schema.org/schema#"
}