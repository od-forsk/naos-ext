{
  "title": "An API token",
  "type": "object",
  "required": [
    "id",
    "token",
    "client_ip",
    "user_agent",
    "created",
    "expires",
    "last_seen_at",
    "user_id"
  ],
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid",
      "readOnly": true
    },
    "token": {
      "type": "string",
      "readOnly": true
    },
    "client_ip": {
      "type": "string",
      "readOnly": true
    },
    "user_agent": {
      "type": "string",
      "readOnly": true
    },
    "created": {
      "type": "string",
      "format": "date-time",
      "readOnly": true
    },
    "expires": {
      "type": "string",
      "format": "date-time",
      "readOnly": true
    },
    "last_seen_at": {
      "type": "string",
      "format": "date-time",
      "readOnly": true
    },
    "user_id": {
      "type": "string",
      "format": "uuid",
      "readOnly": true
    }
  },
  "$schema": "http://json-schema.org/schema#"
}