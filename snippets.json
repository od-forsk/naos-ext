{
    "NAOS User": {
        "prefix": "user",
        "body": [
            "{",
            "    \"login\": \"${1}\",",
            "    \"name\": \"${2}\",",
            "    \"email\": \"${3}\",",
            "    \"is_admin\": ${4|false,true|}$0",
            "}"
        ],
        "description": "NAOS User"
    },
    "NAOS Team": {
        "prefix": "team",
        "body": [
            "{",
            "    \"name\": \"${1}\",",
            "    \"allocated_naos_tokens\": ${2}$0",
            "}"
        ],
        "description": "NAOS Team"
    },
    "NAOS Instance": {
        "prefix": "instance",
        "body": [
            "{",
            "  \"parameters\": {${1}},",
            "  \"name\": \"${2:notneeded}\"$0",
            "}"
        ],
        "description": "NAOS Instance"
    },
    "NAOS Project": {
        "prefix": "project",
        "body": [
            "{",
            "  \"name\": \"${1:name}\",",
            "  \"source\": {",
            "    \"dbname\": \"${2:dbname}\",",
            "    \"host\": \"${3:host}\",",
            "    \"port\": ${4:5432},",
            "    \"schema\": \"${5:public}\",",
            "    \"type\": \"${6|PostgreSQL,Oracle,MS_SQL_Server,Atl|}\"",
            "  }$0",
            "}"
        ],
        "description": "NAOS Project"
    },
    "NAOS Workspace": {
        "prefix": "workspace",
        "body": [
            "{",
            "  \"name\": \"${1:name}\",",
            "  \"project_id\": \"${2:project}\"$0",
            "}"
        ],
        "description": "NAOS Workspace"
    },
    "NAOS Job": {
        "prefix": "job",
        "body": [
            "{",
            "  \"name\": \"${1}\",",
            "  \"work_area_type\": \"${2|project,workspace|}\",",
            "  \"work_area_id\": \"${3}\",",
            "  \"tasks\": [",
            "    {",
            "      \"function_name\": \"${4}\",",
            "      \"name\": \"${4}\",",
            "      \"link\": ${5|false,true|},",
            "      \"input\": {$0}",
            "    }",
            "  ]",
            "}"
        ],
        "description": "NAOS Job"
    },
    "NAOS Geofile": {
        "prefix": "geofile",
        "body": [],
        "description": "NAOS Geofile"
    },
    "NAOS Coverage": {
        "prefix": "coverage",
        "body": [],
        "description": "NAOS Coverage"
    }
}